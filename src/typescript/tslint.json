{
  "extends": "tslint-config-airbnb",
  "rules": {
    "no-any": false,
    "semicolon": ["error", "never"],
    "arrow-parens": [false]
  },
  "jsRules": {
    "max-line-length": {
      "options": [ 80 ]
    },
    "complexity": ["error", 20],
    "max-len": ["error", 120],
    "max-params": ["error", 6],
    "max-statements": ["error", 25],
    "no-use-before-define": 0
  }
}
